<html><head>  <title>Brew Blager</title>  <link rel="stylesheet" href="http://netdna.bootstrapcdn.com/bootstrap/3.2.0/css/bootstrap.min.css">    <script src="//code.jquery.com/jquery-1.11.0.min.js"></script>    <script src="http://netdna.bootstrapcdn.com/bootstrap/3.1.1/js/bootstrap.min.js"></script>  <script src="https://ajax.googleapis.com/ajax/libs/angularjs/1.5.7/angular.min.js"></script>  <script src="https://cdnjs.cloudflare.com/ajax/libs/angular-ui-router/0.3.2/angular-ui-router.js"></script>  <link href="http://maxcdn.bootstrapcdn.com/font-awesome/4.1.0/css/font-awesome.min.css" rel="stylesheet">  <script src="/javascripts/bootstrap-rating-input.min.js"></script>  <link rel="icon" href="/images/mug.ico">  <script src="/javascripts/angularApp.js"></script>   <link rel="stylesheet" href="/stylesheets/style.css">  <style> .glyphicon-thumbs-up { cursor:pointer } </style>    </head><body ng-app="app">    <nav class="navbar navbar-default navbar-inverse">  <div class="container-fluid">    <!-- Brand and toggle get grouped for better mobile display -->    <div class="navbar-header">      <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#bs-example-navbar-collapse-1" aria-expanded="false">        <span class="sr-only">Toggle navigation</span>        <span class="icon-bar"></span>        <span class="icon-bar"></span>        <span class="icon-bar"></span>      </button>      <a class="navbar-brand">BrewBlager</a>    </div>    <!-- Collect the nav links, forms, and other content for toggling -->    <div class="collapse navbar-collapse" id="bs-example-navbar-collapse-1">      <ul class="nav navbar-nav">        <li><a href="/">Home</a></li>        <li><a href="/#/test">Catalog</a></li>        <li><a href="/#/login">About Me</a></li>      </ul>            <ul class="nav navbar-nav navbar-right">        <li><a href="/#/login">Login</a></li>        <li><a href="/#/login">Sign Up</a></li>      </ul>      <form class="navbar-form navbar-right">        <div class="form-group">          <input type="text" class="form-control" placeholder="Search">        </div>        <button type="submit" class="btn btn-default">Submit</button>      </form>    </div><!-- /.navbar-collapse -->  </div><!-- /.container-fluid --></nav><!--....................... HEADER ...................-->  <div class="parallax">  </div>  <div class = "jumbotron">    <img src="https://d30y9cdsu7xlg0.cloudfront.net/png/137618-200.png">    <span id="brew">Brew</span><span id="blog">Blager</span></h1>  </div><!--................. INSERT TEMPLATES ..................-->  <div class="row templates">    <div class="col-md-8 col-md-offset-2">    	<ui-view></ui-view>    </div>  </div>  <!---:::::::::::::      TEMPLATES     ::::::::::::::--><!--................. HOMEPAGE .....................-->  <script type = "text/ng-template"   id="/home.html">   <div class="page-header">    <h1>Welcome back!</h1>   </div>    <p>What new brew did you try today?</p>        <form ng-submit="addPost()"    style="margin-top:30px;">    <div class="form-group">      <input type="text"      class="form-control"      placeholder="Name"      ng-model="name"></input>    </div>    <div class="form-group">      <input type="text"      class="form-control"      placeholder="Content"      ng-model="content"></input>    </div>    <div class="form-group">      <input type="number"      min="1"      max="10"      class="form-control"      placeholder="Rating"      ng-model="rating"></input>    </div>    <button type="submit" class="btn btn-primary">Post</button>  </form>      <div class="row">        <div ng-repeat="post in posts | orderBy:'-rating'">                  <div class="col-sm-6 col-md-4">            <div class="thumbnail">              <img src="http://drizly.com/blog/wp-content/uploads/2015/06/xbigstock-Ale-Beer-73578616-600x450.jpg.pagespeed.ic.m1VtsciPc3.jpg">                            <div class="caption">              <span class="glyphicon glyphicon-heart" ng-click="incrementRating(post)">{{post.rating}}/10</span>                <h3>{{post.name}}</h3>                <p>{{post.content}}</p>                <p><a href="#/posts/{{$index}}" class="btn btn-default" role="button">View Post</a></p>              </div>            </div>          </div>        </div></script><!--................. VIEW POST PAGE .....................--><script type="text/ng-template" id="/posts.html">  <div class="page-header">    <h3>      <a ng-show="post.link" href="{{post.link}}">        {{post.name}}      </a>      <span ng-hide="post.link">        {{post.name}}      </span>    </h3>  </div>     <p>      <span ng-show="post.pic">        {{post.pic}}      </span>      <span ng-show="post.content">        {{post.content}}      </span>  </p>  <div ng-repeat="comment in post.comments | orderBy:'-upvotes'">    <span class="glyphicon glyphicon-thumbs-up"      ng-click="incrementUpvotes(comment)"></span>    {{comment.upvotes}} - by {{comment.author}}    <span style="font-size:20px; margin-left:10px;">      {{comment.body}}    </span>  </div>  <form ng-submit="addComment()"    style="margin-top:30px;">    <h3>Add a new comment</h3>    <div class="form-group">      <input type="text"      class="form-control"      placeholder="Comment"      ng-model="body"></input>    </div>    <button type="submit" class="btn btn-primary">Post</button>  </form></script><!--................. LOGIN PAGE .....................--> <script type = "text/ng-template" id="/login.html">   <div class ="page-header"><h1>Login Page</h1></div>        <form ng-submit=""    style="margin-top:30px;">    <h3>Welcome back! Sign in below</h3>    <div class="form-group">      <input type="text"      class="form-control"      placeholder="Email"      ng-model="email"></input>    </div>    <div class="form-group">      <input type="text"      class="form-control"      placeholder="Password"      ng-model="password"></input>    </div>       <button type="submit" class="btn btn-primary">Post</button>  </form>  </div></script> <script type = "text/ng-template" id="/test.html"><div class = "page-header"><h1><span class="glyphicon glyphicon-plus-sign"></span>  Add a new review</h1>  </div>  <form style="margin-top:30px;" ng-submit="addBeer()">    <div class = "row">    <div class = "col-lg-6 col-md-6 col-sm-12">     <h4>Basic Info</h4>     <center>      <div class="form-group">        <input type="text"        class="form-control input-lg"        placeholder="Name"        ng-model="name" required></input>      </div>        <div class="form-group">        <select type="text"         class="form-control input-lg"        ng-model="style" required>        <option value="" selected disabled>Style</option>        <option ng-repeat="style in styles | orderBy: 'name'">{{style.name}}</option>        </select>      </div>      <div class="form-group">        <span class="input-group-addon " id="basic-addon-1"><button type="button" class="btn btn-lg"><span class="glyphicon glyphicon-minus-sign"></span></button></span>                <input type="text" class="form-control input-lg"  placeholder="ABV" ng-model="ABV" aria-describedby="basic-addon-2 basic-addon-1" required></input>         <span class="input-group-addon " id="basic-addon-2"><button type="button" class="btn btn-lg"><span class="glyphicon glyphicon-minus-sign"></span></button></span>      </div>      <div class="form-group">        <input type="text"        class="form-control input-lg"        placeholder="Brewery"        ng-model="brewery" required></input>      </div><button class="btn btn-default btn-link optional" type="button" data-toggle="collapse" data-target="#collapseExample" aria-expanded="false" aria-controls="collapseExample"> <p>Advanced</p> </button>    <div class="collapse" id="collapseExample">                 <div class="form-group">        <input type="text"        class="form-control input-lg"        placeholder="When is it available?"        ng-model="availability" optional></input>      </div>      <div class="form-group">        <input type="text"        class="form-control input-lg"        placeholder="Where did you find it?"        ng-model="location" optional></input>      </div>      </div></div>      <div class = "col-md-6">     <h4>What do you think?</h4>      <div class = "form-group">        <input type="number"        data-min="1" data-max="5"        class="rating"        ng-model="rating" id="rating" data-empty-value="1" data-icon-lib="fa"         data-active-icon="fa-star fa-3x" data-inactive-icon="fa-star-o fa-3x"         required>        </input>        </div>      <div class = "form-group">        <textarea type="text" style="height:155px;"        class="form-control input-lg"        placeholder="Description"        ng-model="description" required>      </textarea>      </div>      </div></div>      <div class = "row">   <button type="submit" class="btn btn-lg" id="postBtn">Post</button>   </div>      </form>  <div class = "page-header"><h1><span class="glyphicon glyphicon-th-list"></span>Recent reviews</h1></div>        <div ng-repeat="beer in beers | orderBy:'-rating'">                  <div class="col-sm-6 col-md-6">            <div class="thumbnail">              <img src="https://warontherocks.com/wp-content/uploads/2015/10/beer-349876_1920.jpg"/>                            <div class="caption">                <span display="inline"><h3>{{beer.name}}<repeat n={{beer.rating}}></repeat></h3>                </span>                <p>                <table style="width:100%">                                       <tr>                      <th>Style</th>                      <td>{{beer.style}}</td>                    </tr>                    <tr>                      <th>ABV</th>                      <td>{{beer.ABV}}</td>                    </tr>                    <tr>                      <th>Brewery</th>                      <td>{{beer.brewery}}</td>                    </tr>                    <tr>                      <th>Description</th>                      <td>{{beer.description}}</td>                    </tr>                    <tr>                      <th>Availability</th>                      <td>{{beer.availability}}</td>                    </tr>                    <tr>                      <th>Location</th>                      <td>{{beer.location}}</td>                    </tr>                                       </table>                </p>                <p><a href="#/posts/{{$index}}" class="btn btn-default" role="button">View Post</a></p>              </div>            </div>          </div>        </div>  </script>                </body></html>